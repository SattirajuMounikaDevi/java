import java.util.*;
class StringReverse
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
String s,rev=" ";
System.out.println("enter String:-");
s=sc.nextLine();
String[] st= s.split(" ");
for (int i=0; i<st.length; i++) 
{
String str = st[i];
String rev1 = "";
for (int j =str.length()-1; j >= 0; j--) 
 {
  rev1 = rev1 + str.charAt(j);
  }
  rev = rev+ " "+rev1;
  } 
     System.out.println(rev);
}
}
