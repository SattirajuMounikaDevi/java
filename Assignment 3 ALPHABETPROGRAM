import  java.util.*;
class Pattern

{

    public static void main(String args[])

    {
 
       int i,j,n;
Scanner sc=new Scanner(System.in);
n=sc.nextInt();

char ch='A';      
for( i=1;i<=n;i++)
         
        {
      
       for( j=1;j<=i;j++)
       
        {
          
       System.out.print(ch+" ");
ch++;
        }
      
System.out.println(" ");
         }
   }

}
