import java.util.*;

class Vowel_Consonant

{
    
public static void main(String args[])

 {
    char ch;
  
    Scanner sc=new Scanner(System.in);
 
    ch=sc.next().charAt(0);
  
    switch(ch)

     {
      
         case 'a':
      
         case 'e':
          
         case 'i':
           
         case 'o':

         case 'u':System.out.println("vowel");
    
                     break;

         default:
 
           System.out.println("consonant");      
            
   
     }

  }

}
