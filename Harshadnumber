import java.util.*;
class Harshad
{
 public static void main(String[] args) 
{
  Scanner sc=new Scanner(System.in);
    int n;
     n=sc.nextInt();
     int n2=n;
     int sum=0;
    while(n!=0)
    {
        int rem=n%10;
        n=n/10;
        sum=sum+rem;
    }
    if(n2%sum==0)
     System.out.println(" Harshad Number");
    else
     System.out.println("not a Harshad Number");
  sc.close();
}
}
