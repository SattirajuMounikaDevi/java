import java.util.*;
class Links
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
System.out.println("Enter elements in to list");
int n=sc.nextInt();
LinkedList<Integer> d=new LinkedList<Integer>();
for(int i=0;i<n;i++)
{
d.add(sc.nextInt());
}
System.out.print(d);
int l=sc.nextInt();
if(d.contains(l))
{
int value=d.indexOf(l);
System.out.print(value);
System.out.println("Removed Element is:"+d.remove(sc.nextInt()));
System.out.print("Remaining list is:"+d);
}
else
{
System.out.print("Not Found");
}
}
}

