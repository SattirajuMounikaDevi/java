import java.util.*;
class Product
{
public static void main(String args[])
{
int n,i,j,sum=0,sum1=0;
Scanner sc=new Scanner(System.in);
n=sc.nextInt();
int a[]=new int[n];
System.out.println("enter elements-");
for(i=0;i<n;i++)
{
a[i]=sc.nextInt();
}
for(i=1;i<n;i++)
sum=sum+a[i];
sum++;
for(j=0;i<n/2;j++)
{
a[j]=sc.nextInt();
}
for(j=i+1;j<n/2;j++)
sum1=sum1+a[j];
sum1++;
int x=sum*sum1;
System.out.println(x);
}
}
