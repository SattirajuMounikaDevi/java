import java.util.*;
class Palindrome
{
public static void main(String args[])
{
String  str,m;
Scanner sc=new Scanner(System.in);
System.out.println("enter String:");
str=sc.nextLine();
int n=str.length();
for(int i=n-1;i>=0;i--)
 m=m+str.charAt(i);
if(str.equals(m))
System.out.println("Palindrome");
else
System.out.println("not a palindrome");
}
}
