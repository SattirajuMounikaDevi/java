https://leetcode.com/problems/plus-one/submissions/


class Solution {
    public int[] plusOne(int[] digits)
    {
       int i,n;
       n=digits.length;
    for(i=n-1;i<n;i--)
    {
        if(digits[i]<=9)
        {
            digits[i]++;
            return digits;
        }
        else
            digits[i]=0;
        
    }
        return digits;
    }
}
